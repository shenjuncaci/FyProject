@{
    ViewBag.Title = "HistoryList";
    Layout = null;
}
<link href="~/Content/Styles/learunui-framework.css" rel="stylesheet" />
<link href="~/Content/Styles/learunui-accordion.css" rel="stylesheet" />
<script src="~/Content/Scripts/jquery/jquery-1.8.2.min.js"></script>
<script src="~/Content/Scripts/learunui-framework.js"></script>
<link href="~/Content/Styles/buttons.css" rel="stylesheet" />

<div id="test">
    111
</div>


<script>
    $(document).ready(function () {

        var htmltext = "<ul>";
        var arry = '@ViewData["Result"]'.split('|');
        var temp;
        for (var i = 0; i < arry.length; i++)
        {
            temp = arry[i].split(',');
            htmltext += "<li id=" + temp[0] + " onClick=Confirm(this.id)>" + temp[1]+" "+"第"+i+"版"+ "</li>";
        }

        htmltext += "</ul>";
        //alert(htmltext);
        document.getElementById("test").innerHTML = htmltext;
    });

    function Confirm(id)
    {
        window.returnValue = id;
        window.close();
    }
</script>
