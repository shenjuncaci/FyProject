    //搜索
    function btn_Search() {
        var StartDt = $("#StartDt").val();
        var EndDt = $("#EndDt").val();
        var Content = $("#Content").val();
        var ResponseBy = $("#ResponseBy").val();
        var ResponseDept = $("#ResponseDept").val();
        var keywords = "";
        if (IsNullOrEmpty(StartDt))
        {
            keywords += " and cast(res_cdate as date)>=cast('"+StartDt+"' as date) ";
        }
        if (IsNullOrEmpty(EndDt))
        {
            keywords += " and cast(res_cdate as date)<=cast('" + EndDt +"' as date) ";
        }
        if (IsNullOrEmpty(Content))
        {
            keywords += " and res_ms like '%" + Content + "%' ";
        }
        if (IsNullOrEmpty(ResponseBy))
        {
            keywords += " and realname like '%" + ResponseBy + "%' ";
        }
        if (IsNullOrEmpty(ResponseDept))
        {
            keywords += " and fullname like '%" + ResponseDept + "%' ";
        }
        
        
        $("#gridTable").jqGrid('setGridParam', {
            url: "@Url.Content("~/CommonModule/Rapid/GridPageListJson")?keywords=" + escape(keywords) + "&CompanyId=" + CompanyId + "&DepartmentId=" + DepartmentId, page: 1
        }).trigger('reloadGrid');
    }


